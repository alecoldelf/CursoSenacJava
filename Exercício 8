    import java.util.Scanner;

public class Main
{
	public static void main(String[] args) {
		
		Scanner sc = new Scanner (System.in);
		
		int n1 = 0;
		
		int n2 = 0;
		
		int n3 = 0;
		
		int n4 = 0;
		
		System.out.println("São quantosd eleitores? ");
		n1 = sc.nextInt();
		
		System.out.println("Quantos votos foram em branco? ");
		n2 = sc.nextInt();
		
		System.out.println("Quantos votos foram nulos? ");
		n3 = sc.nextInt();
		
		System.out.println("Quantos votos foram válidos? ");
		n4 = sc.nextInt();
		
		int n5 = n2+n3+n4;
		
		System.out.println("O número total de votos foi de: " + n5);
		
		System.out.println("A porcentagem de votos nulos é de: " + (n3*100/n5));
		
		System.out.println("A porcentagem de votos válidos é de: " + (n4*100/n5));
		
		System.out.println("A porcentagem de votos em branco é de: " + (n2*100/n5));
	}
}
