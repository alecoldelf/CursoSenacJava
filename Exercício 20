package cap8e9;

import java.util.*;

public class Ex20{
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner (System.in);
		
		float x = 0;
		
		float y = 0;	
		
		System.out.println("Escolha um número aleatório: ");
		x = sc.nextInt();
		
		System.out.println("Escolha um número aleatório: ");
		y = sc.nextInt();
		
		if (x>y) {
			System.out.println(x + " e " + y);
		}
		
		else {
		    System.out.println(y + " e 50" + x);
		}
	}
}
