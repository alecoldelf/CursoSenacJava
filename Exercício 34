package cap8;

import java.util.*;

public class Ex34 {
	public static void main(String[] args) {
	
		Scanner sc = new Scanner (System.in);
	
		int x;
	
		int y;

		System.out.println("Digite o primeiro número: ");
		x = sc.nextInt();
		
		System.out.println("Digite o segundo número: ");
		y = sc.nextInt();	
		
		float z = ((x*y)+5);
		
		if (z<=0) {
			System.out.println("O número é menor ou igual a zero");
		}
	
		else if (z<=100) {
			System.out.println("O número é menor ou igual a 100, mas maior que 0");
		}
		
		else {
			System.out.println("O número é maior ou igual a 101");
		}
	}
}
