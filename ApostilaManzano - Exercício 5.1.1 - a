    import java.util.Scanner;
    
    

public class Main
{
	public static void main(String[] args) {
		
		Scanner sc = new Scanner (System.in);
		
		System.out.println("Digite um n√∫mero: ");
		int num = sc.nextInt();
		int cont = 1;
		
		while (cont <= 10) {
		    int mult = num*cont;
		    System.out.println(cont + "X" + num + " = " + mult);
		    cont++;
		}
		
		sc.close();
	}
}
