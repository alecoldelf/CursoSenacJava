package cap8e9;

import java.util.*;

public class Ex19{
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner (System.in);
		
		float x = 0;
		
		float y = 0;
		
		System.out.println("Escolha um número aleatório: ");
		y = sc.nextInt();
		
		System.out.println("Escolha um número aleatório: ");
		y = sc.nextInt();
		
		if (x>y) {
			System.out.println("O Maior deles é o: " + x);
		}
		
		else {
		    System.out.println("O Maior deles é o: " + y);
		}
	}
}
