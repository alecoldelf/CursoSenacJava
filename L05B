package l05;

	import java.util.Scanner;

public class B
{
public static void main(String[] args) {
	
	Scanner sc = new Scanner (System.in);
	
	System.out.println("Digite um n√∫mero: ");
	int num = sc.nextInt();
	
	for (int cont = 1; cont <= 10; cont++) {
	    int mult = num*cont;
	    System.out.println(cont + "X" + num + " = " + mult);
	}
	
	sc.close();
}
}
