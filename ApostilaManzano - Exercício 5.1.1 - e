package manzano;

	import javax.swing.*;

public class e 
{
    public static void main(String[] args) 
    {
        String total="";
         int i = 0;
         int j = 0;
         int n3 = 3;
         
         while (i<15) {
        	for(i=1;i<=15;i++)
         {
            n3=n3*3;
            total=total+n3+",";
         }  
         System.out.println(total);
    } 
         }
         
}
